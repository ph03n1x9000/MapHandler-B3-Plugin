<configuration plugin="maphandler">
	<settings name="commands">
		<!-- Specify what admin level can use the commands here -->
		<set name="firstcycle-oc">20</set>
		<set name="secondcycle-sc">20</set>
		<set name="thirdcycle-tc">20</set>
		<set name="admincycle-ac">20</set>
		<set name="resetcycle-rc">20</set>
	</settings>
	<settings name="settings">
		<!--Enter the names of the files with the map list-->
		<set name="mapfile1">mapcycle.txt</set>
		<set name="mapfile2">mapcycle2.txt</set>
		<set name="mapfile3">mapcycle3.txt</set>
		<!--The next cycle can only be called by an admin, the plugin will not use it automatically -->
		<set name="admin_mapfile">admin_cycle.txt</set>
        	<!-- amount of players that must be on server before map cycle changes -->
		<set name="player_amount2">6</set>
		<set name="player_amount3">10</set>
	</settings>
	<settings name="options">
		<!-- Set the cvars that will change when a new mapcycle is in effect.-->
		<set name="cycle2_options">g_gravity 300, g_fraglimit 250</set>
		<set name="cycle3_options">g_gravity 900, g_fraglimit 300</set>
		<set name="original_options">g_gravity 800, g_fraglimit 150</set>
	</settings>
</configuration>
